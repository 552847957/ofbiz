              <parameter param-name="partyId" from-field="parameters.partyId"/>
            </show-portlet>
        </menu-item>
        <menu-item name="PartyAttributes" title="${uiLabelMap.PartyAttributes}">
            <condition>
              <or>
                  <if-empty field="PartyAttributesMenu"/>
                  <if-compare field="PartyAttributesMenu" operator="equals" value="Y"/>
              </or>
            </condition>
